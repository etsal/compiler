        elif n < 0: begin return: prime(-n) end
        elif n < 0: begin return: prime(-n) end
        else:
            begin
            i := 3
            loop:
                begin
                if i > n /2: begin break end
                elif n% i = 0: begin return:false end
                i := i + 2
